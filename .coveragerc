[run]
branch = True
source = src
omit =
    */tests/*
    */site-packages/*
    */__pycache__/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod
    class .*\bProtocol\):
    @typing.no_type_check
    @overload

precision = 2
show_missing = True
skip_covered = False

[html]
directory = htmlcov
